{
  "name": "generator-arts-api",
  "version": "1.0.1",
  "main": "server.ts",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon -r ts-node --esm -r tsconfig-paths/register server.ts",
    "generate": "ts-node --esm -r tsconfig-paths/register src/modules/engine/src/main.ts",
    "build": "tsc && tsc-alias",
    "build:watch": "concurrently \"tsc -w\" \"tsc-alias -w\"",
    "start": "node dist/main.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier --check .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "google-auth-library": "^8.5.2",
    "joi": "^17.6.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.6.4",
    "multer": "^1.4.5-lts.1",
    "ts-node": "^10.9.1",
    "canvas": "2.10.1",
    "cli-color": "^2.0.3",
    "gif-encoder-2": "^1.0.5",
    "sha-1": "1.0.0",
    "sha1": "^1.1.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/jest": "^29.0.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.36",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.1",
    "tsc-alias": "^1.6.9",
    "typescript": "^4.8.3"
  }
}
