{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "incremental": true,
    "moduleResolution": "node",
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./*"],
      "@/public": ["../public/*"]
    }
  },
  "include": ["**/*.js", "**/*.jsx"]
}
